  <select class='form-control' name='DO' id='select-DO'>
    <option value=''>도/시</option>
    <% var DOs = ['서울', '인천', '대전', '대구', '광주광역시', '부산광역시', '울산', '세종특별자치시', '경기도', '강원도', '충청북도', '충청남도', '경상북도', '경상남도', '전라북도', '전라남도', '제주특별자치도'] %>
    <% for (var i = 0; i < DOs.length; i++) { %>
    <option value='<%=DOs[i]%>' <% if (DO == DOs[i]) { %>selected<% } %> ><%= DOs[i] %></option>
    <% } %>
  </select>
